{
  "name": "Profile",
  "theme": {
    "selector": {
      "background": "#1b1b1e",
      "title_buttons": "#28282d",
      "heading": "#FFD700",
      "buttons": "#A6A6A6",
      "buttons_hover": "#D3D3D3",
      "workspace_name": "#9966CC",
      "workspace_path": "#A6A6A6",
      "workspace_hover": "#444444",
      "last_accessed": "#A6A6A6",
      "workspace_background": "#28282d",
      "text": "#28282d"
    },
    "marketplace_selector": {
      "background": "#1b1b1e",
      "heading": "#FFD700",
      "buttons": "#A6A6A6",
      "buttons_hover": "#D3D3D3",
      "marketplace_name": "#9966CC",
      "marketplace_url": "#A6A6A6",
      "marketplace_hover": "#444444",
      "marketplace_background": "#28282d",
      "text": "#28282d"
    },
    "index": {
      "background": "#1b1b1e",
      "status_bar": "#28282d",
      "status_bar_text": "#A6A6A6",
      "status_bar_hover": "#444444",
      "status_bar_font_size": "13px"
    },
    "prompt": {
      "background": "#1b1b1e",
      "heading": "#FFD700",
      "text": "#A6A6A6",
      "input_background": "#28282d",
      "input_text": "#A6A6A6",
      "input_border": "#A6A6A6",
      "submit_text": "#A6A6A6",
      "submit_background": "#444444"
    }
  },
  "status_bar": [
    {
      "type": "text",
      "content": "${getProfileName()}"
    },
    {
      "type": "button",
      "content": "Open Settings",
      "action": "openSettings()"
    },
    {
      "type": "button",
      "content": "Open Marketplace",
      "action": "openMarketplace()"
    },
    {
      "type": "button",
      "content": "Open Workspace Selector",
      "action": "openSelector()"
    }
  ]
}
